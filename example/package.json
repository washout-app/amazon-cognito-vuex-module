{
  "name": "vue-webpack-starter",
  "version": "0.0.0",
  "description": "Minimal webpack-based seed project for Vue applications",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Botre/vue-webpack-starter"
  },
  "browserslist": [
    "last 2 versions",
    "ie > 8"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.config.js --hot",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.config.js --progress",
    "serve": "npm run build && http-server dist -o --gzip",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "vue": "^2.5.12"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.1.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.6",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.6.0",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "node-sass": "^4.7.2",
    "portfinder": "^1.0.13",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.12",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1",
    "webpack-merge": "^4.1.1"
  }
}
